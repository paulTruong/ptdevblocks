					"gradients": [],
					"palette": [
						{
							"color": "#11192e",
							"name": "Foreground",
							"slug": "foreground"
						},
						{
							"color": "#ffffff",
							"name": "Background",
							"slug": "background"
						},
						{
							"color": "#52191e",
							"name": "Primary",
							"slug": "primary"
						}
					],
					"text": true
				},
				"layout": {
					"contentSize": false,
					"wideSize": false
				},
				"shadow": {
					"defaultPresets": true,
					"presets": []
				},
				"spacing": {
					"customSpacingSize": true,
					"spacingScale": {
						"increment": 1.5,
						"mediumStep": 1.5,
						"operator": "*",
						"steps": 7,
						"unit": "rem"
					},
					"spacingSizes": []
				},
				"typography": {
					"customFontSize": true,
					"dropCap": true,
					"fontFamilies": [
						{
							"fontFace": [
								{
									"fontFamily": "Cartridge",
									"fontStretch": "normal",
									"fontStyle": "normal",
									"fontWeight": "400",
									"src": [
										"file:./assets/fonts/cartridge/Cartridge-Regular.woff2"
									]
								},
								{
									"fontFamily": "Cartridge",
									"fontStretch": "normal",
									"fontStyle": "normal",
									"fontWeight": "700",
									"src": [
										"file:./assets/fonts/cartridge/Cartridge-Bold.woff2"
									]
								}
							],
							"fontFamily": "Cartridge, sans-serif",
							"name": "Cartridge",
							"slug": "cartridge"
						},
						{
							"fontFamily": "georgia, serif",
							"name": "Georgia",
							"slug": "georgia"
						}
					],
					"fontSizes": [
						{
							"name": "Small",
							"size": "clamp(0.88rem, calc(0.83rem + 0.23vw), 1.00rem)",
							"slug": "small"
						},
						{
							"name": "Medium",
							"size": "clamp(1.00rem, calc(0.91rem + 0.45vw), 1.25rem)",
							"slug": "medium"
						},
						{
							"name": "Large",
							"size": "clamp(1.25rem, calc(1.02rem + 1.14vw), 1.88rem)",
							"slug": "large"
						},
						{
							"name": "Extra Large",
							"size": "clamp(1.50rem, calc(0.95rem + 2.73vw), 3.00rem)",
							"slug": "xlarge"
						},
						{
							"name": "2X Large",
							"size": "clamp(1.88rem, calc(0.92rem + 4.77vw), 4.50rem)",
							"slug": "xxlarge"
						},
						{
							"name": "Huge",
							"size": "clamp(2.25rem, calc(0.80rem + 7.27vw), 6.25rem)",
							"slug": "huge"
						}
					],
					"fontStyle": true,
					"fontWeight": true,
					"letterSpacing": true,
					"textDecoration": true,
					"textTransform": true
				}
