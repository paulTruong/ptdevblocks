{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/button": {
				"appearanceTools": true,
				"color": {
					"background": true,
					"custom": true,
					"customDuotone": true,
					"customGradient": true,
					"defaultDuotone": true,
					"defaultGradients": true,
					"defaultPalette": true,
					"duotone": [],
					"gradients": [],
					"palette": [
						{
							"color": "#11192e",
							"name": "Foreground",
							"slug": "foreground"
						},
						{
							"color": "#ffffff",
							"name": "Background",
							"slug": "background"
						},
						{
							"color": "#ffd72d",
							"name": "Primary",
							"slug": "primary"
						}
					],
					"text": true
				},
				"layout": {
					"contentSize": false,
					"wideSize": false
				},
				"shadow": {
					"defaultPresets": true,
					"presets": []
				},
				"spacing": {
					"customSpacingSize": true,
					"spacingScale": {
						"increment": 1.5,
						"mediumStep": 1.5,
						"operator": "*",
						"steps": 7,
						"unit": "rem"
					},
					"spacingSizes": []
				},
				"typography": {
					"customFontSize": true,
					"dropCap": true,
					"fontFamilies": [
						{
							"fontFace": [
								{
									"fontFamily": "Cartridge",
									"fontStretch": "normal",
									"fontStyle": "normal",
									"fontWeight": "400",
									"src": [
										"file:./assets/fonts/cartridge/Cartridge-Regular.woff2"
									]
								},
								{
									"fontFamily": "Cartridge",
									"fontStretch": "normal",
									"fontStyle": "normal",
									"fontWeight": "700",
									"src": [
										"file:./assets/fonts/cartridge/Cartridge-Bold.woff2"
									]
								}
							],
							"fontFamily": "Cartridge, sans-serif",
							"name": "Cartridge",
							"slug": "cartridge"
						},
						{
							"fontFamily": "georgia, serif",
							"name": "Georgia",
							"slug": "georgia"
						}
					],
					"fontSizes": [],
					"fontStyle": true,
					"fontWeight": true,
					"letterSpacing": true,
					"textDecoration": true,
					"textTransform": true
				}
			},
			"core/media-text": {
