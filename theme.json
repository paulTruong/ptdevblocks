					"slug": "secondary"
				},
				{
					"color": "#ff8b48",
					"name": "Accent",
					"slug": "accent"
				}
			]
		},
		"layout": {
			"contentSize": "46.25rem",
			"wideSize": "75rem"
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Cartridge",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/cartridge/Cartridge-Regular.woff2"
							]
						},
						{
							"fontFamily": "Cartridge",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/cartridge/Cartridge-Bold.woff2"
							]
						}
					],
					"fontFamily": "Cartridge, sans-serif",
					"name": "Cartridge",
					"slug": "cartridge"
				},
				{
					"fontFamily": "georgia, serif",
					"name": "Georgia",
					"slug": "georgia"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "clamp(0.88rem, calc(0.83rem + 0.23vw), 1.00rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.00rem, calc(0.91rem + 0.45vw), 1.25rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.25rem, calc(1.02rem + 1.14vw), 1.88rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(1.50rem, calc(0.95rem + 2.73vw), 3.00rem)",
					"slug": "xlarge"
				},
				{
					"name": "2X Large",
					"size": "clamp(1.88rem, calc(0.92rem + 4.77vw), 4.50rem)",
					"slug": "xxlarge"
				},
				{
					"name": "Huge",
					"size": "clamp(2.25rem, calc(0.80rem + 7.27vw), 6.25rem)",
					"slug": "huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "100px"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"letterSpacing": "0.5px"
				}
			},
			"core/image": {
				"border": {
					"radius": "8px"
				}
			},
			"core/media-text": {
				"spacing": {
					"padding": {
						"left": "0",
						"right": "0"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"letterSpacing": "0.5px",
					"textDecoration": "none"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "0.5px",
					"textDecoration": "none"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--xxlarge)",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--xlarge)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.1"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"h4": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--georgia)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.3",
			"textTransform": "none"
		}
	},
	"version": 2
}