{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
            "gradients": [
                {
                    "slug": "main-gradient",
                    "gradient": "linear-gradient(90deg, #FCC934 0%, #FE5E23 25%, #FB202F 50%, #B61D3C 75%, #6C2240 100%)",
                    "name": "Main Gradient"
                }
            ],
            "palette": [
                {
                    "color": "#242424",
                    "name": "Foreground",
                    "slug": "foreground"
                },
                {
                    "color": "#ffffff",
                    "name": "Background",
                    "slug": "background"
                },
                {
                    "color": "#FCC934",
                    "name": "Yellow",
                    "slug": "yellow"
                },
                {
                    "color": "#FE5E23",
                    "name": "Orange",
                    "slug": "orange"
                },
                {
                    "color": "#FB202F",
                    "name": "Bright Red",
                    "slug": "bright-red"
                },
                {
                    "color": "#B61D3C",
                    "name": "Dark Red",
                    "slug": "dark-red"
                },
                {
                    "color": "#6C2240",
                    "name": "Maroon",
                    "slug": "maroon"
                }
            ]
		},
		"layout": {
			"contentSize": "46.25rem",
			"wideSize": "75rem"
		},
        "spacing": {
            "spacingSizes": [
                {
                    "name": "8px (0.5rem)",
                    "size": "0.5rem",
                    "slug": "10"
                },
                {
                    "name": "16px (1rem)",
                    "size": "1rem",
                    "slug": "20"
                },
                {
                    "name": "32px (2rem)",
                    "size": "2rem",
                    "slug": "30"
                },
                {
                    "name": "48px (3rem) - Fluid",
                    "size": "clamp(2.00rem, calc(1.71rem + 1.43vw), 3.00rem)",
                    "slug": "40"
                },
                {
                    "name": "64px (4rem) - Fluid",
                    "size": "clamp(2.00rem, calc(1.43rem + 2.86vw), 4.00rem)",
                    "slug": "50"
                },
                {
                    "name": "96px (6rem) - Fluid",
                    "size": "clamp(4.00rem, calc(3.43rem + 2.86vw), 6.00rem)",
                    "slug": "60"
                },
                {
                    "name": "192px (12rem) - Fluid",
                    "size": "clamp(3.00rem, calc(0.43rem + 12.86vw), 12.00rem)",
                    "slug": "70"
                }
            ]
        },
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Cartridge",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/cartridge/Cartridge-Regular.woff2"
							]
						},
						{
							"fontFamily": "Cartridge",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/cartridge/Cartridge-Bold.woff2"
							]
						}
					],
					"fontFamily": "Cartridge, sans-serif",
					"name": "Cartridge",
					"slug": "cartridge"
				},
				{
					"fontFamily": "georgia, serif",
					"name": "Georgia",
					"slug": "georgia"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "clamp(0.88rem, calc(0.83rem + 0.23vw), 1.00rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.00rem, calc(0.91rem + 0.45vw), 1.25rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.25rem, calc(1.02rem + 1.14vw), 1.88rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(1.50rem, calc(0.95rem + 2.73vw), 3.00rem)",
					"slug": "xlarge"
				},
				{
					"name": "2X Large",
					"size": "clamp(1.88rem, calc(0.92rem + 4.77vw), 4.50rem)",
					"slug": "xxlarge"
				},
				{
					"name": "Huge",
					"size": "clamp(2.25rem, calc(0.80rem + 7.27vw), 6.25rem)",
					"slug": "huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "8px"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"letterSpacing": "0.5px"
				}
			},
			"core/image": {
				"border": {
					"radius": "8px"
				}
			},
			"core/media-text": {
				"spacing": {
					"padding": {
						"left": "0",
						"right": "0"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"letterSpacing": "0.5px",
					"textDecoration": "none"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "0.5px",
					"textDecoration": "none"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--xxlarge)",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--xlarge)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.1"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"h4": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cartridge)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--georgia)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.3",
			"textTransform": "none"
		}
	},
	"version": 2
}
